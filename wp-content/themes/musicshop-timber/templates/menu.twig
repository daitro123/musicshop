{% if menu or accountMenu %}
	<ul>
	{% for item in items %}
		<li class="{{ item.classes | join(' ') }}">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>

				{% if item.children %}
					<div class="submenu-wrapper">
						{% include "submenu.twig" with {'items': item.children} %}
					</div>
				{% endif %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
