{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper container ">
		<div class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="cart-content">
				<h1 class="cart-h1">{{post.title}}</h1>
				<div class="cart-body">
					{{post.content}}
				</div>
			</section>
		</div>
	</div>
	<!-- /content-wrapper -->
{% endblock %}
